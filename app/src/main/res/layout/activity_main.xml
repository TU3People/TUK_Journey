<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <androidx.drawerlayout.widget.DrawerLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/drawer">
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.fragment.app.FragmentContainerView
                android:id="@+id/map"
                android:name="com.naver.maps.map.MapFragment"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:navermap_latitude="37.340174"
                app:navermap_longitude="126.7335933" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2">
                <ImageView
                    android:id="@+id/topbackimage"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/backgg"
                    android:scaleType="centerCrop"
                    android:adjustViewBounds="true" />
            <androidx.appcompat.widget.Toolbar
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/roundback"
                android:layout_margin="15dp"
                android:padding="0dp"
                android:id="@+id/toolbar">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textColor="#858585"
                    android:text="검색어를 입력하세요"
                    android:textSize="18dp"
                    />
            </androidx.appcompat.widget.Toolbar>
            </FrameLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_weight="5"
                android:background="@drawable/topround"
                android:layout_marginTop="-20dp"
                android:id="@+id/moveframe">
                <FrameLayout
                    android:id="@+id/handlewrapper"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginLeft="80dp"
                    android:layout_marginRight="80dp">
                    <View
                        android:id="@+id/handlebar"
                        android:layout_width="80dp"
                        android:layout_height="6dp"
                        android:layout_gravity="top|center_horizontal"
                        android:layout_marginTop="12dp"
                        android:background="@drawable/handle_bar"/>
                </FrameLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_marginTop="15dp"
                        android:layout_weight="1">
                        <LinearLayout
                            android:id="@+id/matzipl"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_margin="20dp"
                            >
                            <ImageButton
                                android:id="@+id/matzip"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="40dp"
                                android:background="@drawable/rectangletest"
                                android:clipToOutline="true"
                                android:contentDescription="이미지 버튼"
                                android:layout_gravity="center"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="24dp"
                                android:text="맛집"
                                android:layout_gravity="center"
                                android:gravity="center"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/cafel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_margin="20dp"
                            >
                            <ImageButton
                                android:id="@+id/cafe"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="40dp"
                                android:background="@drawable/rectangletest"
                                android:clipToOutline="true"
                                android:contentDescription="이미지 버튼"
                                android:layout_gravity="center"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="24dp"
                                android:text="카페"
                                android:layout_gravity="center"
                                android:gravity="center"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/activityl"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_margin="20dp"
                            >
                            <ImageButton
                                android:id="@+id/activity"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="40dp"
                                android:background="@drawable/rectangletest"
                                android:clipToOutline="true"
                                android:contentDescription="이미지 버튼"
                                android:layout_gravity="center"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="24dp"
                                android:text="액티비티"
                                android:layout_gravity="center"
                                android:gravity="center"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_marginTop="15dp"
                        android:layout_weight="1">
                        <LinearLayout
                            android:id="@+id/sharel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_margin="20dp"
                            >
                            <ImageButton
                                android:id="@+id/share"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="40dp"
                                android:background="@drawable/rectangletest"
                                android:clipToOutline="true"
                                android:contentDescription="이미지 버튼"
                                android:layout_gravity="center"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="24dp"
                                android:text="바우처 공유"
                                android:layout_gravity="center"
                                android:gravity="center"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/call"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_margin="20dp"
                            >
                            <ImageButton
                                android:id="@+id/cal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="40dp"
                                android:background="@drawable/rectangletest"
                                android:clipToOutline="true"
                                android:contentDescription="이미지 버튼"
                                android:layout_gravity="center"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="24dp"
                                android:text="계산기"
                                android:layout_gravity="center"
                                android:gravity="center"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/spinl"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_margin="20dp"
                            >
                            <ImageButton
                                android:id="@+id/spin"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="40dp"
                                android:background="@drawable/rectangletest"
                                android:clipToOutline="true"
                                android:contentDescription="이미지 버튼"
                                android:layout_gravity="center"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="24dp"
                                android:text="돌림판"
                                android:layout_gravity="center"
                                android:gravity="center"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/timetablel"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="vertical"
                        android:layout_marginTop="15dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        >
                        <ImageButton
                            android:id="@+id/timetable"
                            android:layout_width="315dp"
                            android:layout_height="wrap_content"
                            android:padding="40dp"
                            android:background="@drawable/rectangletest"
                            android:clipToOutline="true"
                            android:contentDescription="이미지 버튼"
                            android:layout_gravity="center"

                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="24dp"
                            android:text="시간표"
                            android:layout_gravity="center"
                            android:gravity="center"/>
                    </LinearLayout>
                </LinearLayout>

            </FrameLayout>
        </LinearLayout>
        </FrameLayout>
        <LinearLayout
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:background="#FFD2D2"
            android:layout_gravity="start"
            android:orientation="vertical"
            android:padding="15dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:orientation="horizontal"
                android:layout_marginTop="50dp"
                >
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="테스트"
                    android:layout_weight="1"
                    android:gravity="center"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="테스트2"
                    android:layout_weight="1"
                    android:gravity="center"/>
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="테스트3"/>
        </LinearLayout>
    </androidx.drawerlayout.widget.DrawerLayout>


</LinearLayout>